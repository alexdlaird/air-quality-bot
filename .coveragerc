[run]
omit =
    devserver.py
    venv/**
    */tests/**
    */lib/**

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    if settings\.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

[xml]
output = build/coverage/coverage.xml

[html]
title = air-quality-bot Coverage Report
directory = build/coverage